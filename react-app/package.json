{
  "name": "prophet-ai-soc-platform",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "build:production": "node scripts/build-production.js",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,css}\"",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:coverage:html": "vitest run --coverage --coverage.reporter=html",
    "test:coverage:json": "vitest run --coverage --coverage.reporter=json",
    "test:coverage:lcov": "vitest run --coverage --coverage.reporter=lcov",
    "test:watch": "vitest --watch",
    "test:components": "vitest run src/components/",
    "test:hooks": "vitest run src/hooks/",
    "test:context": "vitest run src/context/",
    "test:utils": "vitest run src/utils/",
    "test:smoke": "vitest run --grep \"@smoke\"",
    "test:critical": "vitest run --grep \"@critical\"",
    "test:performance": "vitest run --grep \"@performance\"",
    "test:a11y": "vitest run --grep \"@a11y\"",
    "test:debug": "vitest run --reporter=verbose",
    "test:ci": "vitest run --coverage --reporter=json --reporter=html",
    "test:help": "node scripts/test-commands.js help",
    "analyze": "node scripts/analyze-build.js",
    "build:analyze": "npm run build && npm run analyze",
    "test:build": "node scripts/test-production-build.js",
    "build:test": "npm run build && npm run test:build",
    "test:final": "node scripts/final-production-test.js",
    "test:compatibility": "node scripts/test-browser-compatibility.js",
    "deploy:check": "node scripts/deployment-readiness.js"
  },
  "dependencies": {
    "chart.js": "^4.5.0",
    "react": "^19.1.1",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^19.1.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@testing-library/jest-dom": "^6.7.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^22.10.5",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@typescript-eslint/eslint-plugin": "^8.39.1",
    "cross-env": "^7.0.3",
    "@typescript-eslint/parser": "^8.39.1",
    "@vitejs/plugin-react": "^5.0.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "jsdom": "^26.1.0",
    "prettier": "^3.6.2",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.39.1",
    "vite": "^7.1.2",
    "vitest": "^3.2.4"
  }
}
